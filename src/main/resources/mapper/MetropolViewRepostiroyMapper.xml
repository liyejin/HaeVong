<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.heabong.web.repository.MetropolViewRepostiory">
	<select id="findAll">
		select * from metropol_view order by id
	</select>
	<select id="findByMetropolId">
		select * from metropol_view where id = #{metropolId}
	</select>
	
</mapper>
